{
    "name": "@agentic/triage-trackers",
    "version": "1.0.0",
    "description": "Issue tracker integrations - GitHub, Jira, Linear, Beads",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./github": {
            "types": "./dist/github.d.ts",
            "import": "./dist/github.js"
        },
        "./jira": {
            "types": "./dist/jira.d.ts",
            "import": "./dist/jira.js"
        },
        "./linear": {
            "types": "./dist/linear.d.ts",
            "import": "./dist/linear.js"
        },
        "./beads": {
            "types": "./dist/beads.d.ts",
            "import": "./dist/beads.js"
        },
        "./octokit": {
            "types": "./dist/octokit.d.ts",
            "import": "./dist/octokit.js"
        }
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "typecheck": "tsc --noEmit"
    },
    "keywords": ["github", "jira", "linear", "beads", "issue-tracker"],
    "author": "Jon Bogaty <jon@jonbogaty.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/agentic-dev-library/triage.git",
        "directory": "packages/trackers"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@agentic/triage-ai": "workspace:*",
        "@ai-sdk/mcp": "^0.0.1",
        "ai": "^6.0.0",
        "octokit": "^5.0.5"
    },
    "optionalDependencies": {
        "@linear/sdk": "^68.1.0"
    },
    "devDependencies": {
        "@types/node": "^22.10.2",
        "tsup": "^8.5.0",
        "typescript": "^5.9.3"
    }
}
