{
    "name": "@agentic/triage-core",
    "version": "1.0.0",
    "description": "Core types, schemas, and utilities for agentic triage",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./schemas": {
            "types": "./dist/schemas/index.d.ts",
            "import": "./dist/schemas/index.js"
        }
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "typecheck": "tsc --noEmit"
    },
    "keywords": ["triage", "core", "schemas", "types"],
    "author": "Jon Bogaty <jon@jonbogaty.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/agentic-dev-library/triage.git",
        "directory": "packages/core"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@agentic/triage-ai": "workspace:*",
        "@agentic/triage-trackers": "workspace:*",
        "@ai-sdk/mcp": "^0.0.1",
        "@playwright/test": "^1.49.1",
        "ai": "^6.0.0",
        "octokit": "^5.0.5",
        "zod": "^4.2.1"
    },
    "devDependencies": {
        "@types/node": "^22.10.2",
        "tsup": "^8.5.0",
        "typescript": "^5.9.3"
    }
}
